{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"  // Only handle GET for SSE
      ],
      "route": "crawl"  // Keep the route as /crawl
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ],
  "scriptFile": "index.js",
  "entryPoint": "default"
}
